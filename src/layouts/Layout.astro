---
import Header from "../components/Navbar/navbar.astro";

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/kacero-logo.jpg" />
		<meta name="generator" content={Astro.generator} />
		<title>Kacero - {title}</title>
		<link rel="stylesheet" href="/src/styles/normalize.css" />
	</head>
	<body>
		<Header />
		<slot />
	</body>
</html>
